      "source": [
        "import cvxpy as cp\n",
        "x1, x2 = cp.Variable(), cp.Variable()\n",
        "obj = cp.Maximize( 20*x1 + 60*x2 )\n",
        "cons = [5*x1 + 4*x2 <= 80,\n",
        "        2*x1 + 4*x2 <= 40,\n",
        "        2*x1 + 8*x2 <= 64,\n",
        "        x1 >= 0,\n",
        "        x2 >= 0]\n",
        "P = cp.Problem(obj, cons)\n",
        "P.solve(verbose=True)\n",
        "print(x1.value, x2.value)"
      ],
      "execution_count": 3,
